<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/product.css">
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>

            <main>
                <!-- Show Clients/Suppliers navigation -->
                <div class="admin__navigation">
                    <div class="client__container btn">
                        <div class="clientImg__container">
                            <img src="https://cdn-icons-png.flaticon.com/512/6009/6009864.png" alt="">
                        </div>
                        <a href="/admin/clients">Show Clients</a>
                    </div>
                    <div class="supplier__container btn">
                        <div>
                            <img src="data:image/png;base64," alt="supplierImg">
                        </div>
                        <a href="/admin/suppliers">Show Suppliers</a>
                    </div>
                </div>
                <br>
                <!-- Add Supplier btn -->
                <div class="centered">
                    <a class="btn" href="/admin/add-supplier">Add Supplier</a>
                </div>
                <!-- Suppliers Data -->
                <% if (data.length > 0) { %>
                    <div class="grid">
                        <% for (let product of data) { %>
                            <article class="card product-item">
                                <header class="card__header">
                                    <h1 class="product__title">
                                        <%= product.supplier_name %>
                                    </h1>
                                    <h5 class="centered"> <%= product.email %></h5>
                                </header>
                                <div class="card__image">
                                    <% if(!product.imageurl) { %>
                                        <img src="data:image/png;base64," alt="supplierImg">
                                    <% } else { %>
                                        <img src="<%= product.imageurl %>" alt="<%= product.client_name %>">
                                    <% } %>
                                </div>
                                <div class="card__content">
                                    <h3 class="product__price">
                                        <%= product.country %>
                                    </h3>
                                    <h3 class="centered">Country Code:
                                        <%= product.countryCode %>
                                    </h3>
                                    <div class="number_of_orders">
                                        <h2>
                                        <%= product.number_of_orders %>
                                        </h2>
                                    </div>
                                    <h2 class="phone_number">tel: +359 
                                        <%= product.phone_number %>
                                    </h2>
                                    <p class="centered"> Address:
                                        <%= product.address %>
                                    </p>
                                </div>
                                <div class="card__actions">
                                    <a href="/admin/edit-supplier/<%= product.supplier_id %>?edit=true" class="btn">Edit</a>
                                    <form action="/admin/delete-supplier" method="POST">
                                        <input type="hidden" value="<%= product.supplier_id %>" name="supplierId">
                                        <button class="btn danger" type="submit">Delete</button>
                                    </form>

                                </div>
                            </article>
                            <% } %>
                    </div>
                    <% } else { %>
                        <h1 class="centered">You have no Suppliers!</h1>
                        <% } %>
            </main>
            <%- include('../includes/end.ejs') %>